Vec ::: Vec{x: Float, y: Float}

i := 5
f := 3.14159
s := "world"
v := Vec{2.3, 4.5}
l := [[1,2],[3,4]]
l2 := [[Vec{2.,3.}]]

$puts("Hello: i=$i f=$f s=$s v=$v l=$l l2=$l2 expr=$(i + 1)!")
$puts("\n<-newline \x21<-bang \101<-A, backslash: \\")
$puts("Fn: $( (x:Int,y:Int)-> x + y )")

toslice := "Hello world"
char := toslice.1
slice := toslice.[2..6]
$puts("Char: $char Slice: $slice")
$puts("Invalid char: $(toslice.(-10)) Invalid slice: $(toslice.[-100..-5]) Backwards: $(toslice.[#toslice..1])")

$puts("string" + "+" + "concatenation")

if yes
    multiline := """
        Hello world
            (indented x4 here)
        i is $i escape: \x41


        ^^^^ 2x blank line ^^^^^^
        expression: $(1 + 2)
        the end.
    """
    $puts("Multliline:\n$multiline<END>")
