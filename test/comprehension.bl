x := [i*100 for i in 1..10]
$puts "$x"

y := [i*100 for i in 1..999 stop if i > 5]
$puts "$y"

z := [i*100 for i in 1..10 skip if i mod 2 == 0]
$puts "$z"

w := [i*100 for i in 1..10 if i mod 2 == 0]
$puts "$w"

p := [s for i,s in ["a","b","c","d"] if i mod 2 == 0]
$puts "$p"

t := {i,i*i for i in 1.. stop if i > 6}
$puts "$t"

t2 := {v,k for k,v in t}
$puts "$t2"
