a := 3
b := 2
x := 3.5
y := 2.5
$puts("Addition: $(a+b) $(x+y)")
$puts("Subtraction: $(a-b) $(x-y)")
$puts("Multiplication: $(a*b) $(x*y)")
$puts("Division: $(a/b) $(x/y)")
$puts("Modulus: $(a mod b) $(x mod 1.)")
$puts("Negative num modulus: $(-a mod b) $(-x mod 1.)")
$puts("Negative modulus: $(a mod -b) $(x mod -1.)")
$puts("Exponentiation: $(a^b) $(x^y)")

f := 23.5
$puts("$f casts to int: $((f):Int)")
$puts("$f rounds to int: $($float_to_int(f):Int)")

i := 123
$puts("$i casts to float: $((i):Float)")
$puts("$f rounds to float: $($int_to_float(i):Float)")
