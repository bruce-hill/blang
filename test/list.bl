L := [10, 20, 30, 40, 50]
say "L: $L"
say "#L: $(#L)"
x := L[3]
say "L[3]: $x"
L[1] = 999
say "L after setting index 1: $L"

L2 := [1.5, 2.5, 3.5, 4.5]
say "L2: $L2"
x2 := L2[3]
say "L2[3]: $x2"
L2 += 99.5
say "After append: $L2"

L3 := [[10],[20]]
say "Nested: $L3"
say "Inner [2][1]: $((L3[2] or fail)[1])"
(L3[2] or fail)[1] = 999
say "After setting [2][1]: $L3"
// (L3[99] or fail)[1] = 999
// say "Unreachable: $L3"

L4 := [:Int]
say "Empty: $L4"

say "L: $L sliced[2..4]: $(L[2..4])"
L += 9999
say "After append: $L"

a := [1,2,3,4]
b := [1,2,3,4]
say "Full slice: a[..] = $(a[..])"
say "$a == $b? $(a == b)"
say "$a == ($a)[..]? $(a == a[..])"
say "($a)[..] == ($b)[..]? $(a[..] == b[..])"
