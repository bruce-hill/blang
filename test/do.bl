
def sqrt(n:Num):Num?
    return n < 0. ? nil; $sqrt(n):Num

for x in [9.,-1.,-2.,-3.,-999.,4.]
    say "x = $x"
    do
        x = sqrt(x) or skip
        say "First: $x"
    else
        x = sqrt(x+1.) or skip
        say "Second: $x"
    else
        x = sqrt(x+2.) or skip
        say "Third: $x"
    else
        stop if x < -5.
        say "I give up: $x"
