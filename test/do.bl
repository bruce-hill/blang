
def sqrt(n:Num):Num?
    return n < 0. ? nil; $sqrt(n):Num

for x in [9.,-1.,-2.,-3.,-999.,4.]
    $puts "x = $x"
    do
        x = sqrt(x) or skip
        $puts "First: $x"
    else
        x = sqrt(x+1.) or skip
        $puts "Second: $x"
    else
        x = sqrt(x+2.) or skip
        $puts "Third: $x"
    else
        stop if x < -5.
        $puts "I give up: $x"
